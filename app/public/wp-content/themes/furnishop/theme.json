					"size": "15px",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "16px",
					"name": "Medium"
				},
				{
					"slug": "upper-heading",
					"size": "18px",
					"name": "Upper Heading"
				},
				{
					"slug": "normal",
					"size": "clamp(18px,1.6vw,20px)",
					"name": "Normal"
				},
				{
					"slug": "big",
					"size": "clamp(22px,3vw,24px)",
					"name": "Big"
				},
				{
					"slug": "large",
					"size": "clamp(24px,4.2vw,32px)",
					"name": "Large"
				},
				{
